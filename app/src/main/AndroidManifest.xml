<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loyagram.campaignsdkdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.loyagram.campaignsdkdemo.LoyagramApplication">
        <activity android:name="com.loyagram.campaignsdkdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.loyagram.campaignsdkdemo.CampaignActivity" android:configChanges="orientation|screenSize" android:windowSoftInputMode="adjustResize">
        </activity>
    </application>
</manifest>